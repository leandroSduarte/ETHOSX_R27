User Function VL_DEBBXS()
Local lRetLP530:=0
        
IF(SE2->E2_PREFIXO=='GPE'.Or.Alltrim(SE2->E2_TIPO)=="RES" .Or. Alltrim(SE2->E2_TIPO)$"TX|INS|PIS|COF")
   /*/lRetLP530:=SED->ED_DEBITO/*/
   lRetLP530:= posicione("SED",1,xFILIAL("SED")+SE2->E2_NATUREZ,"ED_DEBITO") 
   
ELSE IF(SE2->E2_TIPO='MED') 
   /*/lRetLP530:=SED->ED_CREDIT/*/
   lRetLP530:= posicione("SED",1,xFILIAL("SED")+SE2->E2_NATUREZ,"ED_CREDIT")
ELSE 
   /*/lRetLP530:= posicione(‘SA2’,1,xFilial('SA2')+SE2->E2_FORNECE,’A2_CONTA’) /*/
   lRetLP530:= posicione("SA2",1,xFILIAL("SA2")+SE2->E2_FORNECE+SE2->E2_LOJA,"A2_CONTA")   
EndIf	                 	
Return(lRetLP530)
